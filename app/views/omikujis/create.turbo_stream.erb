<!-- app/views/omikujis/create.turbo_stream.erb -->
<%= turbo_stream.replace "omikuji-result" do %>
  <%= render "omikujis/display", snake: @snake %>
<% end %>

<%= turbo_stream.prepend "public-history-list" do %>
  <%= render partial: "omikujis/history_item", locals: { history: @history } %>
<% end %>


<%= turbo_stream.prepend "my-history-list" do %>
  <%= render partial: "omikujis/history_item", locals: { history: @history } %>
<% end %>
