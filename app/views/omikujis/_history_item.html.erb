<p id="<%= dom_id(omikuji_history) %>" class="omikuji-item">
  <strong class="omikuji-result"><%= omikuji_history.snake.omikuji_result.name %></strong><br>
  <span class="omikuji-description"><%= omikuji_history.snake.omikuji_result.description %></span><br>
  <span class="omikuji-date"><%= omikuji_history.created_at.strftime('%Y-%m-%d %H:%M') %></span>
</p>
